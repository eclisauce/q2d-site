<!DOCTYPE html>
<html>
{{>head}}

<body>
  {{>header}}
  <main>
    <div class="container" id="">
      <div class="row">
        <section class="col-lg-12 col-12 mx-auto">
          {{#with iotSolutions}}
            <div class="row mx-auto">
              <h1 class="mx-auto">{{heading}}</h1>
              <img src="/assets/img/index/startPageFirstPart.jpeg" alt="" class="img-fluid col-12 col-lg-6" style="height: 250px;">
              <div class="col-lg-6 col-12">
                <p>{{textOne}}</p>
                <h4>{{subHeaderOne}}</h4>
                <p>{{textTwo}} <a href="{{linkOne}}" target="_blank">{{linkOneText}}</a></p>
                <p>{{textThree}}</p>
                <h4>{{subHeaderTwo}}</h4>
                <p>{{textFour}}</p>
              </div>
            </div>
          {{/with}}
        </section>
        <section class="col-lg-10 mx-auto col-12 mt-4">
          {{#with LoRaWAN}}
            <div id="accordion">
              <div class="card">
                <div class="card-header bg-dark" id="headingOne" data-toggle="tooltip" title="Tryck fÃ¶r mer information">
                  <h5 class="mb-0 text-center">
                    <button class="btn btn-link text-light" id="accordionBtn" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                      {{btnText}} <i class="fas fa-angle-double-down"></i>
                    </button>
                  </h5>
                </div>

                <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                  <div class="card-body">
                    <h3>{{title}}</h3>
                    <p>{{textOne}}</p>
                    <h4>{{subHeaderOne}}</h4>
                    <ul>
                      {{#each listOne}}
                        <li>{{listText}}</li>
                      {{/each}}
                    </ul>
                    <p>{{textTwo}}</p>
                    <p>{{textThree}}</p>
                    <p>{{textFour}}</p>
                    <ul>
                      {{#each listTwo}}
                        <li>{{listText}}</li>
                      {{/each}}
                    </ul>
                  </div>
                </div>
              </div>

            </div>
          {{/with}}
        </section>
      </div>
      <h4><a href="/product" class="text-dark" id="integrationLink"><i class="fas fa-arrow-left"></i> Tillbaka till produkter</a></h4>

    </div>


  </main>
  {{>footer}} {{>scripts}}
</body>

</html>
