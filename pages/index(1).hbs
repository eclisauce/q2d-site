<div class="col-12 m-1">
  {{#with first_part}}
    <h1 class="mt-2 text-center" id="mainHeader">{{header}}</h1>
    <section>
      <div class="card-deck mt-4 mx-auto">
        {{#each cards}}
          <div class="card border-0">
            <h2 class="text-center">{{header}}</h2>
            <a href="{{link}}" data-toggle="tooltip" title="Mer information"><img class="card-img-top img-fluid" src="/assets/img/{{image}}" alt="Card image cap"></a>
            <div class="card-body">
              <h5 class="card-title">{{title}}</h5>
              <p class="card-text">{{text}}</p>
              <a href="{{link}}" class="btn btn-dark text-light" data-toggle="tooltip" title="Mer information">{{linkText}}</a>
            </div>
          </div>
        {{/each}}
      </div>
    </section>
  {{/with}}
  <div class="row">
    {{#with second_part}}
      <h1 class="text-center mb-2 mt-2">{{header}}</h1>
      <section class="mt-4 p-1">
        <div class="card-deck">
          {{#each cards}}
            <div class="card border-0 " id="workshopCards">
              <a href="{{link}}" data-toggle="tooltip" title="Mer information"><img class="card-img-top img-fluid" src="/assets/img/{{image}}" alt="Card image cap"></a>
              <div class="card-body">
                <h4 class="card-title">{{title}}</h4>
                <p class="card-text">{{text}}</p>
              </div>
              <div class="card-footer border-0">
                <a href="{{link}}" class="btn btn-dark text-light" data-toggle="tooltip" title="Mer information">{{linkText}}</a>
              </div>
            </div>
          {{/each}}
        </div>
      </section>
    {{/with}}
  </div>
  <div class="row">
    {{#with third_part}}
      <section class="m-1">
        <h2 class="mt-5 display-5">{{header}}</h2>
        {{#each items}}
          <h4 class="mt-3">{{heading}}</h4>
          <ul>
            <div class="card border-0 float-right col-4 d-none d-md-block" id="part_three-card">
              <img class="img-fluid" id="part_three-img" src="/assets/img/{{image}}" alt="">
            </div>
            {{#each texts}}
              <li>{{text}}</li>
            {{/each}}
          </ul>
        {{/each}}
        {{#each itemsTwo}}
          <h4>{{heading}}</h4>
          {{#each texts}}
            <p>{{text}}</p>
          {{/each}}
        {{/each}}
      </section>
    {{/with}}
  </div>
</div>
